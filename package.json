{
  "name": "touro-guide-app",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "description": "Cross-platform native app for Touro guides",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",

    "publish-staging": "expo publish --release-channel staging",
    "publish-prod": "expo publish --release-channel prod-v1",

